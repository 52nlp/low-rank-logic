# Chart settings
#set title "Averaged 11-point Precision/Recall"
set key right top
set key width 0.5
set key box

load 'Set1.plt'

set terminal pdf enhanced linewidth 3.0  size 3.000000,2.00000
set output "results.pdf"
# XYChart settings
set nologscale


#set style line 12 lc rgb '#bbbbbb' lt 1 lw 1
#set style line 13 lc rgb '#cccccc' lt 4 lw 1
#set grid xtics mxtics ytics mytics back ls 12, ls 13


set yrange [0.0:1]
set xrange [0.0:1]
set ytics 0.2,0.2,1
set xtics 0.0,0.1,1

#set mytics 2
#set mxtics 2


set xr [0.0:1.0] noreverse
